<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="ErrorReporterTool.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="ErrorReporterTool.commands.category"
            id="ErrorReporterTool.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="ErrorReporterTool.commands.sampleCommand"
            class="org.wso2.developerstudio.eclipse.errorreporter.ui.menu.SampleHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="ErrorReporterTool.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="ErrorReporterTool.menus.sampleMenu">
            <command
                  commandId="ErrorReporterTool.commands.sampleCommand"
                  mnemonic="S"
                  id="ErrorReporterTool.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="ErrorReporterTool.toolbars.sampleToolbar">
            <command
                  commandId="ErrorReporterTool.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="ErrorReporterTool.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.wso2.developerstudio.eclipse.errorreporter.startup.Startup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.wso2.developerstudio.eclipse.errorreporter.ui.prefs.PreferencePage"
            id="org.wso2.developerstudio.eclipse.errorreporter.page1"
            name="Developer Studio Error Reporting">
      </page>
   </extension>

</plugin>
